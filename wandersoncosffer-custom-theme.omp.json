{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#f36943",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÇ∂",
          "style": "diamond",
          "template": " {{ .UserName }} ",
